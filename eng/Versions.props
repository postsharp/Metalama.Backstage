<Project>

    <!-- Version of PostSharp.Backstage -->
    <Import Project="MainVersion.props" />
    
    <PropertyGroup>
        <BackstageVersion>$(MainVersion)$(PackageVersionSuffix)</BackstageVersion>
        <BackstageAssemblyVersion>$(MainVersion)</BackstageAssemblyVersion>
    </PropertyGroup>

    <!-- Versions of dependencies -->
    <PropertyGroup>
    </PropertyGroup>

    <!-- Overrides by local settings -->
    <Import Project="../artifacts/private/Backstage.version.props" Condition="Exists('../artifacts/private/Backstage.version.props')" />
    <Import Project="Dependencies.props" Condition="Exists('Dependencies.props')" />

    <!-- Other properties dependent on the versions set above. -->
    <PropertyGroup>
        <AssemblyVersion>$(BackstageAssemblyVersion)</AssemblyVersion>
        <Version>$(BackstageVersion)</Version>
    </PropertyGroup>

</Project>
